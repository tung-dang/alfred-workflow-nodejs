{
  "name": "@alfred-wf-node/core",
  "version": "1.1.2",
  "description": "Alfred Workflow NodeJS helper in ES6",
  "author": "Tung Dang",
  "main": "dist/index",
  "types": "lib/index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tung-dang/alfred-workflow-nodejs-next"
  },
  "dependencies": {
    "fuzzy": "^0.1.3",
    "node-osascript": "^2.0.0",
    "node-persist": "^2.1.0"
  },
  "bugs": {
    "url": "https://github.com/tung-dang/alfred-workflow-nodejs-next/issues"
  },
  "homepage": "https://github.com/tung-dang/alfred-workflow-nodejs-next",
  "devDependencies": {
    "@types/mocha": "5.2.3",
    "@types/node": "10.3.6",
    "babel-register": "^6.26.0",
    "chai": "4.1.2",
    "codecov": "^3.0.2",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "proxyquire": "2.0.1",
    "sinon": "6.0.0",
    "ts-node": "6.1.1",
    "typescript": "2.9.2"
  },
  "scripts": {
    "test": "NODE_ENV=testing TESTING=true mocha \"test/**/*-test.ts\" \"src/**/*-test.ts\" --opts mocha.opts",
    "test-watch": "yarn test -- --watch",
    "test-coverage": "rm -rf coverage && nyc --reporter=lcov yarn test",
    "check-coverage": "nyc check-coverage --statement 80 --branches 80 --function 80 --lines 80",
    "report-coverage": "codecov",
    "cleanup": "../../scripts/cleanup.sh",
    "format": "../../scripts/format_files.sh",
    "build": "../../scripts/build.sh",
    "build:live": "../../scripts/build-live.sh",
    "start": "yarn build:live"
  },
  "contributors": [
    {
      "name": "Tung Dang",
      "email": "tung.dang.js@gmail.com",
      "url": "https://github.com/tung-dang"
    },
    {
      "name": "Giang Vo",
      "url": "https://github.com/giangvo"
    }
  ],
  "keywords": [
    "alfred",
    "workflow",
    "nodejs",
    "es6"
  ],
  "bin": {
    "run-node": "../../scripts/run-node"
  },
