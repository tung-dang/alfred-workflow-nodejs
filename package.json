{
  "name": "alfred-workflow-nodejs-next",
  "version": "1.0.5",
  "description": "Alfred Workflow NodeJS helper in ES6",
  "author": "Tung Dang",
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tung-dang/alfred-workflow-nodejs-next"
  },
  "dependencies": {
    "fuzzy": "^0.1.3",
    "keychain": "^1.2.0",
    "node-osascript": "^2.0.0",
    "node-persist": "^2.0.10"
  },
  "bugs": {
    "url": "https://github.com/tung-dang/alfred-workflow-nodejs-next/issues"
  },
  "homepage": "https://github.com/tung-dang/alfred-workflow-nodejs-next",
  "devDependencies": {
    "babel-register": "^6.23.0",
    "chai": "3.5.0",
    "codecov": "^1.0.1",
    "mocha": "3.2.0",
    "nyc": "10.1.2",
    "proxyquire": "1.7.11",
    "sinon": "1.17.7"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules",
    "test": "NODE_ENV=testing TESTING=true mocha \"test/**/*-test.js\" \"src/**/*-test.js\" --compilers js:babel-register",
    "test-watch": "yarn test -- --watch",
    "test-coverage": "rm -rf coverage && nyc --reporter=lcov yarn test",
    "check-coverage": "nyc check-coverage --statement 80 --branches 80 --function 80 --lines 80",
    "report-coverage": "codecov"
  },
  "contributors": [
    {
      "name": "Tung Dang",
